    "author": "Ömer YAZICIOĞLU",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "eslint": "^8.43.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3"
    },
    "files": [
        "/dist/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/oyazicioglu/Qei-Validations.git"
    }
